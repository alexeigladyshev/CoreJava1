package HouseNTech;

public class House {

	int address;
	String houseName;
	
	public void House(int ad, String name){
		
		address = ad;
		houseName = name;
	}
	
	public void watchMovie(){
		
		System.out.println("Watching Pirates of Carribean");
	}
	public void goToSleep(){
		
		System.out.println("Sleeping now");
	}
	public void cookDinner(){
		
		System.out.println("Cooking chicken with broccoli");
	}
	public void houseName(){
		
		System.out.println("The name of the house is "+ houseName);
	}
	
}
